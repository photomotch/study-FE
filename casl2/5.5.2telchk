TELCHK	START
	SUBA	GR2,=0
	JPL	BGN
	JZE	SETSTS
	LD	GR3,TABLE,GR1
	SUBA	GR3,=0
	JZE	FIN
	LAD	GR3,-1
	JUMP	FIN
SETSTS	LD	GR3,TABLE,GR1
	CPA	GR3,CMAX
	JPL	CLS
	ST	GR2,TABLE,GR3
CLS	ST	GR2,TABLE,GR1
	JUMP	FIN
BGN	CPA	GR2,CMAX
	JPL	EXTBGN
	ST	GR1,TABLE,GR2
EXTBGN	ST	GR2,TABLE,GR1
FIN	RET
CMAX	DC	50
TABLE	DS	1
	DS	50
	END
	