MAIN	START
	LAD	GR0,0
	LAD	GR1,PARAM
	CALL	FGSUB
	RET
PARAM	DC	#ABCD
	DC	11
	DC	8
	END
;
FGSUB	START
	PUSH	0,GR2
	PUSH	0,GR3
	ST	GR0,WORK
	LD	GR0,0,GR1
	LAD	GR2,15
	SUBA	GR2,1,GR1
	SLL	GR0,0,GR2
	LAD	GR3,-1
	LD	GR2,2,GR1
	SLL	GR3,0,GR2
	AND	GR3,WORK
	ST	GR3,WORK
	LAD	GR2,16
	SUBA	GR2,2,GR1
	SRL	GR0,0,GR2
	OR	GR0,WORK
	POP	GR3
	POP	GR2
	RET
WORK	DS	1
	END
